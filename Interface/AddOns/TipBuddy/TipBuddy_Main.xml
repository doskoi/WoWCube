	<!-- Parent Tooltip Frame -->
	<Frame name="TipBuddy_Parent_Frame" frameStrata="TOOLTIP" parent="UIParent">
		<Size x="2" y="2"/>
		<Scripts>
			<OnLoad>
				TipBuddy_OnLoad();
